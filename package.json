{
  "name": "nimi-card",
  "version": "0.5.0-beta.0",
  "private": false,
  "dependencies": {
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-app-rewired test --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint \"./src/**/*.{ts,tsx,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "ipfs:build": "cross-env PUBLIC_URL=\".\" yarn build",
    "ipfs:publish": "ipfs-deploy build -p infura -O",
    "prebundle": "rm -rf dist",
    "bundle": "npm run prebundle && rollup -c",
    "prepublishOnly": "npm run lint && npm run build && npm run bundle"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "yarn": "use npm!"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@svgr/rollup": "^6.2.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "https-browserify": "^1.0.0",
    "i18next": "^21.6.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-xhr-backend": "^3.2.2",
    "os-browserify": "^0.3.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.7",
    "react-scripts": "^5.0.1",
    "rollup": "^2.74.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.6.4",
    "url": "^0.11.0"
  },
  "peerDependencies": {
    "react": "^18.1.0",
    "styled-components": "^5.3.5"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "resolutions": {
    "type-fest": "^0.21.3"
  }
}
